<%- include('../partials/header') %>
<table>
    <tbody>
        <% flights.forEach(function(f){ %>
            <tr <%- f.departs < new Date() ? 'class="passed"' : '' %>>
                <td><%= f.airline %></td>
                <td><%= f.airport %></td>
                <td><%= f.flightNo %></td>
                <td><%= f.departs %></td>
                <td><a href="/flights/<%= f._id %>">DETAILS</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<%- include('../partials/footer') %>